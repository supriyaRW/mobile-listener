# Gemini AI API Key for image analysis
GEMINI_API_KEY=your_gemini_api_key_here

# Base URL for the application
# Leave empty to use relative paths (recommended for localhost)
# Set to your domain for production (e.g., https://yourdomain.com)
NEXT_PUBLIC_BASE_URL=

# APK file name (default: app-release.apk)
NEXT_PUBLIC_APK_FILE_NAME=app-release.apk

# Download route path (default: /download)
NEXT_PUBLIC_DOWNLOAD_ROUTE=/download

# API route path for APK download (default: /api/apk)
NEXT_PUBLIC_APK_API_ROUTE=/api/apk
